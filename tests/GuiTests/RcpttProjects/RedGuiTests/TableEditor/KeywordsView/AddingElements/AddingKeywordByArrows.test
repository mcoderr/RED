get-view "Project Explorer" | get-tree | select "RobotProject/TestFile.robot" | double-click
get-editor "TestFile.robot" | get-text-viewer | set-text "*** Test Cases ***\n"
    + "test\n"
    + "    log  7\n"
    + "*** Keyword ***\n"
    + "My KW" 
    

with [get-editor "TestFile.robot"] {
    with [get-text-viewer] {
        set-caret-pos 1 1
        key-type "M1+s"
    }
}    
    
get-editor "TestFile.robot" | get-tab-folder | get-tab-item Keywords | click


//set focus on kw
get-view Outline | get-tree | select "TestFile.robot/Keywords/My KW"


with [get-editor "TestFile.robot"] {
    with [get-canvas -after [get-label "User Keywords"] -index 1] {
    	//set focus on first column
        key-type Down
        key-type Enter -times 2
    }
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
    	//add child KW - line1
        type-text line1
        key-type Enter
    }
    //add all args and comment
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text arg1
        key-type Enter
    }
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text arg2
        key-type Enter
    }
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text arg3
        key-type Enter
    }
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text arg4
        key-type Enter
    }
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text "#com"
        key-type Enter 
    }
    wait 200
    get-editbox -after [get-label "User Keywords"] -index 1 | key-type Enter 
    
    get-canvas -after [get-label "User Keywords"] -index 1 | key-type Enter
    //add kw: line 2
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text line2
        key-type Enter
    }
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text b1
        key-type Enter
    }
    //turn off editing mode, go down and press enter - new line will be added
    get-editbox -after [get-label "User Keywords"] -index 1 | key-type "TRAVERSE_ESCAPE"
    with [get-canvas -after [get-label "User Keywords"] -index 1] {
        //new line is added and focus i set to fist cell
        key-type Down
        key-type Enter
        
        //go to to 3rd column
        key-type Right -times 2
    }
    get-canvas -after [get-label "User Keywords"] -index 1 | key-type Enter
    //add 2. arg
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text c2
        key-type Enter
    }
    //go to column 4 and add arg
    get-editbox -after [get-label "User Keywords"] -index 1 | key-type "TRAVERSE_TAB_NEXT"
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text c4
        key-type Enter
    }
    
    //press Down arrow when cell is in edit mode - new line is added
    get-editbox -after [get-label "User Keywords"] -index 1 | key-type Down
    
    //focus is set of first cell - go to last column
    with [get-canvas -after [get-label "User Keywords"] -index 1] {
        key-type Left
        key-type Down
        key-type Enter
    }
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
    	//add comment to new line
        type-text "#only comment"
        key-type Enter
    }
    wait 200
    get-editbox -after [get-label "User Keywords"] -index 1 | key-type Enter 
    
    //new line 
    get-canvas -after [get-label "User Keywords"] -index 1 | key-type Enter
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text "empty kw"
        key-type Enter
    }
    get-editbox -after [get-label "User Keywords"] -index 1 | key-type "TRAVERSE_ESCAPE"

    //go down and add new main kw
    with [get-canvas -after [get-label "User Keywords"] -index 1] {
        key-type Down -times 2
        key-type Left
        key-type Enter
    }
    get-canvas -after [get-label "User Keywords"] -index 1 | key-type Enter
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text "second kw"
        key-type Enter
    }
    //add arg to kw
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text "${s1}"
        key-type Enter
    }
    //turn off editing cell and go to 5. column
    get-editbox -after [get-label "User Keywords"] -index 1 | key-type "TRAVERSE_ESCAPE"
    with [get-canvas -after [get-label "User Keywords"] -index 1] {
        key-type Right -times 3
        key-type Enter
    }
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text "${s5}"
        key-type Enter
    }
    wait 200
    get-editbox -after [get-label "User Keywords"] -index 1 | key-type Down
    //go down and add new kw
    get-canvas -after [get-label "User Keywords"] -index 1 | key-type Enter
    
    //add log
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text Log
        key-type Enter
    }
    //add value = 12
    with [get-editbox -after [get-label "User Keywords"] -index 1] {
        type-text 12
        key-type Enter
    }
}

get-editor "TestFile.robot" | maximize
compare-row-with-list -file "TestFile.robot" -table "User Keywords" -row 1 -ref_table [list "My KW" "" "" "" "" "" "" "" ]
compare-row-with-list -file "TestFile.robot" -table "User Keywords" -row 2 -ref_table [list "line1" "arg1" "arg2" "arg3" "arg4" "#com" "" "" ]
compare-row-with-list -file "TestFile.robot" -table "User Keywords" -row 3 -ref_table [list "line2" "b1" "" "" "" "" "" "" ]
compare-row-with-list -file "TestFile.robot" -table "User Keywords" -row 4 -ref_table [list "\\" "\\" "c2" "\\" "c4" "" "" "" ]
compare-row-with-list -file "TestFile.robot" -table "User Keywords" -row 5 -ref_table [list "\\" "\\" "\\" "\\" "\\" "\\" "#only comment" "" ]
compare-row-with-list -file "TestFile.robot" -table "User Keywords" -row 6 -ref_table [list "empty kw" "" "" "" "" "" "" "" ]
compare-row-with-list -file "TestFile.robot" -table "User Keywords" -row 7 -ref_table [list "..." "" "" "" "" "" "" "" ]

compare-row-with-list -file "TestFile.robot" -table "User Keywords" -row 8 -ref_table [list "second kw" "${s1}" "\\" "\\" "\\" "${s5}" "" "" ]
compare-row-with-list -file "TestFile.robot" -table "User Keywords" -row 9 -ref_table [list "Log" "12" "" "" "" "" "" "" ]
compare-row-with-list -file "TestFile.robot" -table "User Keywords" -row 10 -ref_table [list "..." "" "" "" "" "" "" "" ]
get-editor "TestFile.robot" | restore

get-editor "TestFile.robot" | get-canvas -after [get-label "User Keywords"] -index 1 | key-type "M1+s"
get-editor "TestFile.robot" | get-tab-folder | get-tab-item Source | click

get-editor "TestFile.robot" | get-text-viewer | get-property text | equals "*** Test Cases ***\n"
    + "test\n"
    + "    log  7\n"
    + "*** Keyword ***\n"
    + "My KW\n"
    + "    line1    arg1    arg2    arg3    arg4    #com\n"
    + "    line2    b1\n"
    + "    \\    \\    c2    \\    c4\n"
    + "    \\    \\    \\    \\    \\    \\    #only comment\n"
    + "    empty kw\n"
    + "second kw\n"
    + "    [Arguments]    ${s1}    \\    \\    \\    ${s5}\n"
    + "    Log    12" | verify-true
    
