        key-type Enter

		//&{myDict}
		type-text "&"
        key-type "M1+SPACE"
        key-type Enter
        wait 300
        key-type Enter
    }
}

with [get-editor "TestFile.robot"] {
    with [get-editbox -after [get-label "Test Cases"] -index 1] {
        type-text "arg $"
        key-type "M1+SPACE"
        key-type Down -times 2
        key-type Enter
        wait 200
        key-type Enter
    }
}

check-test_case-row -file "TestFile.robot" -row 1 -tc "new test" 
check-test_case-row -file "TestFile.robot" -row 2 -tc "And this is my kw"  -c1 "Convert To Bytes" -c2 "input" -c3 "${CURDIR}" -c4 "&{EMPTY}" -c5 "${TEST_NAME}" 
