//open file, select all text in Doc view and press red button to remove all Settings
get-view "Project Explorer" | get-tree | select "RobotProject/res.tsv" | double-click
with [get-editor "res.tsv"] {
    get-tab-folder | get-tab-item Settings | click
    get-section General | get-editbox [$doc_editbox_selector] | set-text-selection -offset 5 -startLine 2 -endLine 0 -endOffset 0
    get-button "Delete Settings section" | click
}
wait 1000
//check if selected text was removed
get-editor "res.tsv" | get-section General | get-editbox [$doc_editbox_selector] | get-property text | equals "" | verify-true
get-button "Save \((Ctrl|M1)\+S\)" | click

//go to Source and check
get-editor "res.tsv" | get-tab-folder | get-tab-item Source | click
get-editor "res.tsv" | get-text-viewer | get-property text | equals "" | verify-true



//============================================== tsv =================================================
//open suite file
get-view "Project Explorer" | get-tree | select "RobotProject/tsv_file.tsv" | double-click

//remove content by backspace
get-editor "tsv_file.tsv" | get-tab-folder | get-tab-item Settings | click
with [get-editor "tsv_file.tsv"] {
    with [get-section General | get-editbox [$doc_editbox_selector]] {
    	click
    	set-focus
    	set-text-offset 0 0
        set-text-offset 10 0
        key-type BackSpace -times 10
        key-type "M1+s"
        wait 1000
    }
    get-tab-folder | get-tab-item Source | click
}
get-editor "tsv_file.tsv" | get-text-viewer | get-property text | equals "*** Settings ***\n"
    + "*** Keywords ***\n"
    + "kw\n"
    + "\tlog\t6" | verify-true
    
//check Source
get-editor "tsv_file.tsv" | get-tab-folder | get-tab-item Source | click
get-editor "tsv_file.tsv" | get-text-viewer | get-property text | equals "*** Settings ***\n"
    + "*** Keywords ***\n"
    + "kw\n"
    + "\tlog\t6" | verify-true
//    
//open .robot file    
get-view "Project Explorer" | get-tree | select "RobotProject/TestFile.robot" | double-click

//remove one item from one section and one item from second section
with [get-editor "TestFile.robot"] {
    get-tab-folder | get-tab-item Settings | click
    with [get-section General | get-editbox [$doc_editbox_selector]] {
    	click
    	set-focus
    	set-text-offset 0 0
        set-text-offset 4 1
        key-type BackSpace -times 4
        wait 200
        set-text-offset 4 4
        key-type BackSpace -times 4
        wait 200
        key-type "M1+s"
        wait 1000
        
    }
    get-tab-folder | get-tab-item Source | click
}

//check if text was merged in Source
get-editor "TestFile.robot" | get-text-viewer | get-property text | equals "*** Setting ***\n"
get-editor "TestFile.robot" | get-text-viewer | get-property text | equals "*** Setting ***\n"
    + "Documentation    doc1    \n"
    + "...    \n"
    + "...    doc3    \n"
    + "...    doc4    \n"
    + "...    \n"
    + "...    doc6\n"
    + "Metadata    nothng\n"
    + "\n"
    + "*** Test Cases ***\n"
    + "test\n"
    + "    log  4\n"
    + "    \n"
    + "    \n"
    + "*** Setting ***"
    + "    \n" | verify-true

//undo changes    
with [get-editor "TestFile.robot"] {
    with [get-text-viewer] {
        set-caret-pos 15 16
        key-type "M1+z"
        key-type "M1+s"
    }
    
}
get-editor "TestFile.robot" | get-tab-folder | get-tab-item Settings | click

try -times 7 -delay 1000 -command{
	get-editor "TestFile.robot" | get-section General | get-editbox [$doc_editbox_selector] | get-property text | equals "doc1\n"
	    + "doc2\n"
	    + "doc3\n"
	    + "doc4\n"
	    + "doc5\n"
	    + "doc6" | verify-true
}    
    
with [get-editor "TestFile.robot"] {       
    //remove all items from second section
    with [get-section General | get-editbox [$doc_editbox_selector]] {
    	click
    	set-focus
        set-text-selection -offset 4 -startLine 5 -endLine 3 -endOffset 0
        key-type BackSpace
    }
    get-tab-folder | get-tab-item Source | click
}

//check that one sectionis visible
get-editor "TestFile.robot" | get-text-viewer | get-property text | equals "*** Setting ***\n"
    + "Documentation    doc1    \n"
    + "...    doc2    \n"
    + "...    doc3\n"
    + "Metadata    nothng\n"
    + "\n"
    + "*** Test Cases ***\n"
    + "test\n"
    + "    log  4\n"
    + "    \n"
    + "    \n"
    + "*** Setting ***"
    + "    \n" | verify-true
    
//undo    
with [get-editor "TestFile.robot"] {
    with [get-text-viewer] {
        set-caret-pos 12 16
        key-type "M1+z"
        key-type "M1+s"
    }
}

get-editor "TestFile.robot" | get-tab-folder | get-tab-item Settings | click

try -times 7 -delay 1000 -command{
	get-editor "TestFile.robot" | get-section General | get-editbox [$doc_editbox_selector] | get-property text | equals "doc1\n"
	    + "doc2\n"
	    + "doc3\n"
	    + "doc4\n"
	    + "doc5\n"
	    + "doc6" | verify-true
}     
with [get-editor "TestFile.robot"] {
    //remove first line
    with [get-section General | get-editbox [$doc_editbox_selector]] {
    	click
    	set-focus
    	set-text-offset 0 0
        set-text-offset 4 0
        key-type BackSpace -times 4
        key-type "M1+s"
        wait 500
    }
    get-tab-folder | get-tab-item Source | click
}

//check thal all wasmerged and first line was removed
get-editor "TestFile.robot" | get-text-viewer | get-property text | equals "*** Setting ***\n"
    + "Documentation    \\    \n"
    + "...    doc2    \n"
    + "...    doc3    \n"
    + "...    doc4    \n"
    + "...    doc5    \n"
    + "...    doc6\n"
    + "Metadata    nothng\n"
    + "\n"
    + "*** Test Cases ***\n"
    + "test\n"
    + "    log  4\n"
    + "    \n"
    + "    \n"
    + "*** Setting ***" | verify-true
    

    
//remove all doc without first line    
with [get-editor "TestFile.robot"] {
    with [get-text-viewer] {
        set-caret-pos 6 13
        select-range 7 12 2 14
        key-type BackSpace
        key-type "M1+s"
        wait 1000
    }
    get-tab-folder | get-tab-item Settings | click
}
try -times 4 -delay 1000 -command{
	//check thate Doc is empty in view
	get-editor "TestFile.robot" | get-section General | get-editbox [$doc_editbox_selector] | get-property text | equals "" | verify-true
}
//add new word
with [get-editor "TestFile.robot"] {
    with [get-section General | get-editbox [$doc_editbox_selector]] {
    	click
    	set-focus
        set-text-offset 0 0
        type-text new
        wait 500
    }
    get-tab-folder | get-tab-item Source | click
}
get-editor "TestFile.robot" | get-text-viewer | get-property text | equals "*** Setting ***\n"
    + "Documentation    new\n"
    + "Metadata    nothng\n"
    + "\n"
    + "*** Test Cases ***\n"
    + "test\n"
    + "    log  4\n"
    + "    \n"
    + "    \n"
    + "*** Setting ***"
    + "    \n" | verify-true
    
//remove all docs    
with [get-editor "TestFile.robot"] {
    with [get-text-viewer] {
        set-caret-pos 2 2
        select-range 2 1 2 21
        key-type BackSpace
        key-type "M1+s"
        wait 1000
    }
    get-tab-folder | get-tab-item Settings | click
}
try -times 6 -delay 1000 -command{
	get-editor "TestFile.robot" | get-section General | get-editbox [$doc_editbox_selector] | get-property text | equals "" | verify-true
}


